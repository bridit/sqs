{
  "name": "bridit/sqs",
  "description": "Brid-IT Modular Micro-Framework (Sqs Queue)",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Gustavo Siqueira",
      "email": "gus@brid-it.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "async-aws/sqs": "^1.6",
    "bridit/core": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "phpstan/phpstan": "^0.12.88"
  },
  "autoload": {
    "psr-4": {
      "Brid\\Sqs\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
